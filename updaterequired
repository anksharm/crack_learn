/* package codechef; // don't place package name! */

import java.util.*;
import java.lang.*;
import java.io.*;

/* Name of the class has to be "Main" only if the class is public. */
class Codechef
{
	public static void main (String[] args) throws java.lang.Exception
	{
    int [] arrayWithDuplicates = {1,2,3,4,5};
    
    System.out.println("^^^^^^" + Arrays.toString(arrayWithDuplicates)); 
    //.toString());
    
    System.out.println(arrayWithDuplicates.length);
    
    System.out.println(checkDuplicates(arrayWithDuplicates));
	}

	public static boolean checkDuplicates(int[] arrayIn)
	{
	 List inputList = Arrays.asList(arrayIn);

	 Set inputSet = new HashSet(inputList);
	 
	 System.out.println(inputList.count());
	 
	if(inputList.size()==inputSet.size()){
	    return true;
	 }
	 return false;
	}
	
}

